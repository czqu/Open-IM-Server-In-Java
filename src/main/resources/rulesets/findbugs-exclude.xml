<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!--参考  http://findbugs.sourceforge.net/manual/filter.html#d0e2318 -->
    <!--   https://stackoverflow.com/questions/47936942/how-do-i-run-findbugs-on-specific-package-ignore-other-packages-in-eclipse -->
    <Match>
        <Package name="net.czqu.openimserver.dao.*"/>
    </Match>
    <Match>
        <Package name="net.czqu.openimserver.dao.pojo.*"/>
    </Match>
    <Match>
        <Package name="net.czqu.openimserver.dao.example.*"/>
    </Match>
    <Match>
        <Package name="net.czqu.openimserver.dao.mapper.*"/>
    </Match>
    <Match>
        <Bug pattern="UPM_UNCALLED_PRIVATE_METHOD"/>
    </Match>
</FindBugsFilter>